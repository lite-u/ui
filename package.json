  "version": "0.0.2",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "DEPRECATED - output-md-doc": "npx typedoc",
    "output-json-doc": "node ./docgen.cjs"
  },
  "author": "",
  "main": "./dist/esm/index.js",
