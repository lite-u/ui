  "Button": {
    "tags": {
      "brief": "A customizable button component that supports size and color variants.",
      "intro": "Renders a theme-aware, styled button using context-based configuration. Supports four sizes (xs, s, m, l) and four variants (primary, neutral, warn, error).",
      "example": "<Button>Default</Button>\n<Button primary xs>Confirm</Button>\n<Button error l>Delete</Button>"
    },
    "props": {
      "xs": {
        "type": "boolean",
        "required": false,
        "defaultValue": null,
        "description": "Extra small button. Sizing depends on theme."
      },
      "s": {
        "type": "boolean",
        "required": false,
        "defaultValue": null,
        "description": "Small button. Sizing depends on theme."
      },
      "m": {
        "type": "boolean",
        "required": false,
        "defaultValue": true,
        "description": "Medium button. Sizing depends on theme."
      },
      "l": {
        "type": "boolean",
        "required": false,
        "defaultValue": null,
        "description": "Large button. Sizing depends on theme."
      },
      "primary": {
        "type": "boolean",
        "required": false,
        "defaultValue": null,
        "description": "If true, applies the primary color style."
      },
      "neutral": {
        "type": "boolean",
        "required": false,
        "defaultValue": true,
        "description": "If true, applies the neutral color style."
      },
      "warn": {
        "type": "boolean",
        "required": false,
        "defaultValue": null,
        "description": "If true, applies the warning color style."
      },
      "error": {
        "type": "boolean",
        "required": false,
        "defaultValue": null,
        "description": "If true, applies the error color style."
      }
    }
  },
